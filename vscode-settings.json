{
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 120],
  "extensions.autoUpdate": true,
  "files.associations": {
    "*.scpt": "applescript"
  },
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.defaultDateFormat": "DD.MM.YYYY HH:mm",
  "gitlens.gitExplorer.location": "explorer",
  "go.formatTool": "gofmt",
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "'Meslo LG M DZ for Powerline'",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "workbench.colorTheme": "Monokai",
  "eslint.packageManager": "yarn",
  "breadcrumbs.enabled": true,
  "editor.renderControlCharacters": true,
  "[html]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "gitlens.views.repositories.location": "explorer",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'SF Mono', Menlo, Monaco, Consolas, monospace",
  "editor.fontWeight": "500",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 5000,
  "editor.find.globalFindClipboard": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.tabCompletion": "on",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.fontAliasing": "auto",
  "window.nativeTabs": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "explorer.confirmDelete": false,
  "search.smartCase": true,
  "debug.inlineValues": true,
  "spellright.language": ["en_US", "de_DE"],
  "spellright.groupDictionaries": false,
  "spellright.addToSystemDictionary": true,
  "spellright.documentTypes": ["markdown", "latex", "plaintext", "git-commit"]
}
